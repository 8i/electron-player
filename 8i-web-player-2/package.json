{
  "name": "8i-web-player",
  "version": "2.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "vite --host 0.0.0.0 --port 1234 --strictPort --clearScreen false",
    "build": "vite build",
    "clean": "cd node_modules/.vite && find . -type f ! -path './basic-ssl/*' -print -delete",
    "copy_draco": "mkdir -p public/draco && cp -r node_modules/three/examples/jsm/libs/draco public/",
    "postinstall": "npm run copy_draco",
    "prettier": "npx prettier **/*.{ts,js,html} src",
    "prettier:check": "pnpm run prettier --check",
    "prettier:fix": "pnpm run prettier:run",
    "prettier:run": "pnpm run prettier --write --log-level warn",
    "prettier:print": "pnpm run prettier:check",
    "lint": "eslint",
    "lint:fix": "pnpm lint --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-semistandard": "^17.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.1.0",
    "prettier": "^3.3.3",
    "prettier-plugin-glsl": "^0.2.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vite-plugin-glsl": "^1.3.0"
  },
  "dependencies": {
    "dashjs": "github:8i/dash.js#eighti-development",
    "filesize": "^10.1.6",
    "fpsmeter": "^0.3.1",
    "immutability-helper": "^3.1.1",
    "mp4box": "github:8i/mp4box.js#encryption-development",
    "react": "^19.2.3",
    "react-device-detect": "^2.2.3",
    "three": "^0.171.0"
  }
}
